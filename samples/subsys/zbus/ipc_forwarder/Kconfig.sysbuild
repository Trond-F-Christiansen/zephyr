source "share/sysbuild/Kconfig"

config CPURAD_BOARD
	string "The board used for remote target cpurad"
	default "nrf54h20dk/nrf54h20/cpurad" if $(BOARD) = "nrf54h20dk"
	default "nrf5340dk/nrf5340/cpunet" if $(BOARD) = "nrf5340dk"
